@model InventoryEntity.Brand.BrandDto
@{
    ViewData["Title"] = "Add Brand";
}

<div class="content">
    <div class="page-header">
        <div class="page-title">
            <h4>Brand ADD</h4>
            <h6>Create new Brand</h6>
        </div>
    </div>
    <div asp-validation-summary="ModelOnly" class="text-danger mb-3"></div>
    <form asp-controller="Brand" asp-action="Create" method="post" enctype="multipart/form-data">
        <div class="card">
            <div class="card-body">
                <div class="row">
                    
                        <partial name="_BrandFormFields" model="Model" />
                        <div class="col-lg-12">
                            @* <a href="javascript:void(0);" class="btn btn-submit me-2">Submit</a> *@
                            <button type="submit" class="btn btn-submit me-2">Submit</button>
                            @* <a class="btn btn-cancel" id="BrandSubmitCancelId">Cancel</a> *@
                        </div>
                   
                </div>
            </div>
        </div>
    </form>
</div>

@section Scripts {
    @{
        <partial name="_ValidationScriptsPartial" />
    }
    <script type="text/javascript">
        $(document).ready(function () {
            
        });

    </script>
}